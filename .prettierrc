{
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "trailingComma": "es5",
  "bracketSpacing": true,
  "printWidth": 80,
  "overrides": [
    {
      "files": "*.css",
      "options": {
        "printWidth": 100
      }
    },
    {
      "files": "*.scss",
      "options": {
        "printWidth": 100
      }
    }
  ]
}
